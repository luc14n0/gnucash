#!/bin/bash -le

# -e above so that if any command has an exit code != 0, this script will
#  exit immediately.
mkdir ourpython_bin
ln -s /usr/bin/python2 ourpython_bin/python
export PATH=/ourpython_bin:"$PATH"
echo path is "$PATH"
echo python version is "$(python --version)"

export GTEST_ROOT=/usr/include/gtest/src GMOCK_ROOT=/usr/include/gmock/src

../commonbuild
